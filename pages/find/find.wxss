/* pages/find/find.wxss */
.title{
  width:100%;
  height:88rpx;
  background: #0dc441;
  text-align: center;
  line-height: 88rpx;
  color: white;
}
.searchbox{
  width:100%;
  height: 64rpx;
  line-height: 64rpx;
  padding: 10rpx;
  display: flex;
  
}
.search-bar{
  width:590rpx;
  height: 40rpx;
  border: 1px solid #aaaaaa;
  border-radius: 6rpx;
  
}
.search-btn{
  height: 56rpx;
  margin: 0 10rpx;
}
.background{
  width:100%;
  height: 12rpx;
  background: #f1f1f1;
}

.item{
  height:148rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #aaaaaa;
  padding: 0 22rpx;
}
.leftpic{
  width: 94rpx;
  height: 94rpx;
}
.right-text{
  flex: 1;
  margin-left: 26rpx;
}
.top{
  display: flex;
  justify-content: space-between;
}
.nickname{
  font-weight: bold;
}
.care{
  margin-right: 30rpx;
}


.footer{
  width:100%;
  height:86rpx;
  position: fixed;
  bottom:0;
  background: white;
  display: flex;
  justify-content: space-around;
}

 .footer navigator{
  display: flex;
  margin-top: 6rpx;
  flex-direction: column;
}
.footer navigator span{
  text-align: center;
}
.icon{
  width:36rpx;
  height:38rpx;
}
.text{
  font-size: 26rpx;
  color: #dddddd;
}
.checked{
  color:#0dc441;
}
.camerabox{
  position: relative;
  width:106rpx;
  height: 106rpx;
}
.camera{
  position: absolute;
  left: 25rpx;
  top: 2rpx;
  width:58rpx;
  height:52rpx;
  z-index: 20;
}
.circle{
  position: absolute;
  left: 0;
  top: -26rpx;
  width:100%;
  height: 106rpx;
} 